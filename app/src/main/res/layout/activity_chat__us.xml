<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context=".Chat_Us">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">



                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="180dp"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="7dp"
                    android:layout_alignParentTop="true"
                    android:background="@drawable/ourteamshape">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        >


                        <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="75dp"
                            android:layout_height="75dp"
                            android:id="@+id/imagekunal"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="15dp"
                            android:src="@drawable/kunal" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Kunal Goyal"
                            android:textStyle="bold"
                            android:textSize="25dp"
                            android:id="@+id/named1"
                            android:layout_below="@+id/imagekunal"
                            android:layout_centerVertical="true"
                            android:layout_centerHorizontal="true"/>


                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Computer Science &amp; Engineering"
                            android:textSize="15dp"
                            android:id="@+id/sub1"
                            android:layout_below="@+id/named1"
                            android:layout_centerVertical="true"
                            android:layout_centerHorizontal="true" />
                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/sub1"
                            android:layout_centerHorizontal="true">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:weightSum="5">
                            <ImageView
                                
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:layout_below="@+id/sub1"
                                android:layout_marginRight="10dp"
                                android:layout_weight="1"
                                android:onClick="mail1"
                                android:src="@drawable/gmaillogo" />

                            <View
                                android:layout_width="1dp"
                                android:layout_height="20dp"
                                android:layout_weight="1"
                                android:background="#000000" />

                            <ImageView
                                
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:layout_below="@+id/sub1"
                                android:layout_centerHorizontal="true"
                                android:layout_marginLeft="10dp"
                                android:layout_toRightOf="@+id/mailone"
                                android:layout_weight="1"
                                android:onClick="linked1"
                                android:src="@drawable/linkedin" />

                            <View
                                android:layout_width="1dp"
                                android:layout_height="20dp"
                                android:layout_marginLeft="7dp"
                                android:layout_weight="1"
                                android:background="#000000" />

                            <ImageView
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:layout_below="@+id/sub1"
                                android:layout_centerHorizontal="true"
                                android:layout_marginLeft="10dp"
                                android:layout_toRightOf="@+id/linkedone"
                                android:layout_weight="1"
                                android:onClick="gt1"
                                android:src="@drawable/github" />
                            </LinearLayout>
                        </RelativeLayout>

                    </RelativeLayout>

                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="180dp"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="7dp"
                    android:layout_alignParentTop="true"
                    android:background="@drawable/ourteamshape">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        >


                        <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="75dp"
                            android:layout_height="75dp"
                            android:id="@+id/imagekunal1"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="15dp"
                            android:src="@drawable/abhay" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Abhay Jain"
                            android:textStyle="bold"
                            android:textSize="25dp"
                            android:id="@+id/named2"
                            android:layout_below="@+id/imagekunal1"
                            android:layout_centerVertical="true"
                            android:layout_centerHorizontal="true"/>


                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Computer Science &amp; Engineering"
                            android:textSize="15dp"
                            android:id="@+id/sub2"
                            android:layout_below="@+id/named2"
                            android:layout_centerVertical="true"
                            android:layout_centerHorizontal="true" />
                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/sub2"
                            android:layout_centerHorizontal="true">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:weightSum="5">
                                <ImageView
                                    
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_below="@+id/sub2"
                                    android:layout_marginRight="10dp"
                                    android:layout_weight="1"
                                    android:onClick="mail2"
                                    android:src="@drawable/gmaillogo" />

                                <View
                                    android:layout_width="1dp"
                                    android:layout_height="20dp"
                                    android:layout_weight="1"
                                    android:background="#000000" />

                                <ImageView
                                    
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_below="@+id/sub2"
                                    android:layout_centerHorizontal="true"
                                    android:layout_marginLeft="10dp"
                                    android:layout_toRightOf="@+id/mailtwo"
                                    android:layout_weight="1"
                                    android:onClick="linked2"
                                    android:src="@drawable/linkedin" />

                                <View
                                    android:layout_width="1dp"
                                    android:layout_height="20dp"
                                    android:layout_marginLeft="7dp"
                                    android:layout_weight="1"
                                    android:background="#000000" />

                                <ImageView
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_below="@+id/sub2"
                                    android:layout_centerHorizontal="true"
                                    android:layout_marginLeft="10dp"
                                    android:layout_toRightOf="@+id/linkedone"
                                    android:layout_weight="1"
                                    android:onClick="gt2"
                                    android:src="@drawable/github" />
                            </LinearLayout>
                        </RelativeLayout>

                    </RelativeLayout>

                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="180dp"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="7dp"
                    android:layout_alignParentTop="true"
                    android:background="@drawable/ourteamshape">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        >


                        <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="75dp"
                            android:layout_height="75dp"
                            android:id="@+id/imagekunal2"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="15dp"
                            android:src="@drawable/saatvik" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Saatvik Saini"
                            android:textStyle="bold"
                            android:textSize="25dp"
                            android:id="@+id/named3"
                            android:layout_below="@+id/imagekunal2"
                            android:layout_centerVertical="true"
                            android:layout_centerHorizontal="true"/>


                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Computer Science &amp; Engineering"
                            android:textSize="15dp"
                            android:id="@+id/sub3"
                            android:layout_below="@+id/named3"
                            android:layout_centerVertical="true"
                            android:layout_centerHorizontal="true" />
                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/sub3"
                            android:layout_centerHorizontal="true">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:weightSum="5">
                                <ImageView
                                    
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_below="@+id/sub3"
                                    android:layout_marginRight="10dp"
                                    android:layout_weight="1"
                                    android:onClick="mail3"
                                    android:src="@drawable/gmaillogo" />

                                <View
                                    android:layout_width="1dp"
                                    android:layout_height="20dp"
                                    android:layout_weight="1"
                                    android:background="#000000" />

                                <ImageView
                                    
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_below="@+id/sub3"
                                    android:layout_centerHorizontal="true"
                                    android:layout_marginLeft="10dp"
                                    android:layout_toRightOf="@+id/mailtwo"
                                    android:layout_weight="1"
                                    android:onClick="linked3"
                                    android:src="@drawable/linkedin" />

                                <View
                                    android:layout_width="1dp"
                                    android:layout_height="20dp"
                                    android:layout_marginLeft="7dp"
                                    android:layout_weight="1"
                                    android:background="#000000" />

                                <ImageView
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_below="@+id/sub3"
                                    android:layout_centerHorizontal="true"
                                    android:layout_marginLeft="10dp"
                                    android:layout_toRightOf="@+id/linkedone"
                                    android:layout_weight="1"
                                    android:src="@drawable/github" />
                            </LinearLayout>
                        </RelativeLayout>

                    </RelativeLayout>

                </RelativeLayout>

        </LinearLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>
